<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ranocoder.my" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ranocoder Portal</name>
    <description>
        Native Android application for the ranocoder portal.
    </description>
    <author email="info@ranocoder.com" href="http://my.ranocoder.com">
        ranocoder
    </author>
    
    <content src="http://my.ranocoder.com/" />
    
    <allow-navigation href="*" />
    
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-targetSdkVersion" value="33" />
</widget>
